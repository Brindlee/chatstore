export async function onRequest(context) {
    const buttonOptions = {
        questionText : "What do you like",
        buttons : [
            {
                buttonLabel :"Pizza",
                buttonValue :"pizza",
                url : "" 
                
            },
            {
                buttonLabel :"Burger",
                buttonValue :"burger",
                url : "" 
                
            }
        ]
    }
    return new Response(JSON.stringify(buttonOptions, null, 2));
}